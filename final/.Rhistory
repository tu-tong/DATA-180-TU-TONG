records <- read.csv('https://raw.githubusercontent.com/rfordatascience/tidytuesday/master/data/2021/2021-05-25/records.csv')
head(records)
summary(records)
case_365 <- filter(records, record_duration>365)
count(case_365)
records %>% arrange(desc(record_duration)) %>% select(player, track, date, record_duration) %>% head(1)
record_time_mean <- records %>%
group_by(track) %>%
summarize(mean_time = mean(time))
rank_mean_time <- arrange(record_time_mean, mean_time)
rank_mean_time
year_records <- mutate(records, year = substr(records$date, 1, 4))
head(year_records)
year_avg_time <- year_records %>%
group_by(year) %>%
summarize(avg_time = mean(time))
year_avg_time
year_avg_time %>%
ggplot(aes(year, avg_time)) +
geom_bar(stat = 'identity')
# Do you notice a trend across years? Are the record times getting faster, slower, or no clear pattern in time? (-2)
records %>%
ggplot(., aes(time, fill = type)) +
geom_histogram(position = 'identity', alpha = .5)
# How do record times compare between Single Lap and Three Lap tracks? (-1)
mutate(records, time_per = ifelse(type == "Three Lap", time/3, time)) %>%
ggplot(., aes(time_per, fill = type)) +
geom_histogram(position = 'identity', alpha =.5)
mutate(records, time_per = ifelse(type == "Three Lap", time/3, time)) %>%
filter(shortcut=="No") %>%
ggplot(., aes(time_per, fill = type)) +
geom_histogram(position = 'identity', alpha = .5)
records %>% group_by(system_played) %>% summarize(mean_time = mean(time))
